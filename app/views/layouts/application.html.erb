<!DOCTYPE html>
<html>
<head>
  <title>siteKick</title>
  <%= stylesheet_link_tag :application %>
  <%= javascript_include_tag :application %>
  <%= csrf_meta_tag %>
</head>
<body>

  <%= render 'layouts/navbar' %>
  <% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" %>
  <% end %>

  <div class='container'>
    <%= yield %>
  </div>
</body>
</html>
